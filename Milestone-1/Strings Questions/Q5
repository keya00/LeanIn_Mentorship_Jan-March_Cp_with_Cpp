//String to Integer (atoi)
class Solution {
public:
    int myAtoi(string str) 
    {
       int result=0;  
        int i=0;   
        int count = 1;  
        if(str[i]=='-') 
        {
            count = -1;
            i++;
        }
    
        while(str[i])
        {
            if(!(str[i]-'0'>=0 && str[i]-'0'<=9))
                return str[i];
            result = result*10 + str[i]-'0';
            i++;
        }
        return result*count;
    }
};
